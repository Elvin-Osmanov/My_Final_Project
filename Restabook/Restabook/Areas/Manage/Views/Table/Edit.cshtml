@model Table
@{
    ViewData["Title"] = "Edit";
    Layout = "~/Areas/Manage/Views/Shared/_Layout.cshtml";
  
}

<div class="container-fluid">
    <h1>Edit</h1>
    <form method="post" enctype="multipart/form-data">
        <div class="form-row">
            <div class="form-group col-md-6">
                <label asp-for="@Model.Date"></label>
                <input type="date" class="form-control" asp-for="@Model.Date">
                <span asp-validation-for="@Model.Date" class="text-danger"></span>
            </div>

        </div>
        <div class="form-row">
            <div class="form-group col-md-6">
                <label asp-for="@Model.StartTime"></label>
                <input type="time" class="form-control" asp-for="@Model.StartTime">
                <span asp-validation-for="@Model.StartTime" class="text-danger"></span>
            </div>

        </div>



        <button type="submit" class="btn btn-primary">Edit</button>
    </form>
</div>

@section Scripts{
    <script src="~/lib/jquery-validation/dist/jquery.validate.min.js"></script>
    <script src="~/lib/jquery-validation-unobtrusive/jquery.validate.unobtrusive.min.js"></script>}

