@model Table
@{
    ViewData["Title"] = "Create";
    Layout = "~/Areas/Manage/Views/Shared/_Layout.cshtml";
    List < Time > times= ViewBag.Times;
    }

<div class="container-fluid">
    <h1>Create</h1>
    <form method="post" enctype="multipart/form-data">
        <div class="form-row">
            <div class="form-group col-md-6">
                <label asp-for="@Model.Date"></label>
                <input type="date" class="form-control" asp-for="@Model.Date">
                <span asp-validation-for="@Model.Date" class="text-danger"></span>
            </div>
           
        </div>
        <div class="form-row">

           
            <div class="form-group col-md-6">
                
                    <label asp-for="@Model.TimeIds">Time</label>
                    <select asp-for="@Model.TimeIds" asp-items="@(new SelectList(times,"Id","StartTime"))" multiple class="form-control">
                    </select>
                
                </div>
            

        </div>

       

        <button type="submit" class="btn btn-primary">Create</button>
    </form>
</div>

