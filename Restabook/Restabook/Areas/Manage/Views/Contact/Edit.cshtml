@model Contact
@{
    ViewData["Title"] = "Edit";
    Layout = "~/Areas/Manage/Views/Shared/_Layout.cshtml";
}

<div class="container-fluid">
    <h2>Edit</h2>
    <form method="post">
        <div class="form-group">
            <label asp-for="@Model.Address"></label>
            <input class="form-control" asp-for="@Model.Address">
            <span asp-validation-for="@Model.Address" class="text-danger"></span>
        </div>
        <div class="form-group">
            <label asp-for="@Model.Phone"></label>
            <input class="form-control" asp-for="@Model.Phone">
            <span asp-validation-for="@Model.Phone" class="text-danger"></span>
        </div>
        <div class="form-group">
            <label asp-for="@Model.SupportEmail"></label>
            <input class="form-control" asp-for="@Model.SupportEmail">
            <span asp-validation-for="@Model.SupportEmail" class="text-danger"></span>
        </div>
        <button type="submit" class="btn btn-primary">Edit</button>
    </form>
</div>


@section Scripts{
    <script src="~/lib/jquery-validation/dist/jquery.validate.min.js"></script>
    <script src="~/lib/jquery-validation-unobtrusive/jquery.validate.unobtrusive.min.js"></script>
}

